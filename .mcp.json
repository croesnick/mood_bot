{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "ssh-nerves": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "ssh-mcp",
        "--",
        "--host=moodbot.run",
        "--user=root",
        "--key=${HOME}/.ssh/id_ed25519"
      ],
      "env": {}
    },
    "moodbot-build": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "ssh-mcp",
        "--",
        "--host=moodbot.build",
        "--port=2222",
        "--user=crntng",
        "--key=${HOME}/.ssh/id_rsa"
      ],
      "env": {}
    }
  }
}